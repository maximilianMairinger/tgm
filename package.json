{
  "scripts": {
    "devApp": "concurrently \"npm run buildDevReplServer\" \"node devServerInit.js --dev repl\"        \"npm run buildDevApp\"      \"npm run buildDevSw\"          -n \"serv,node,_app,sewo\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgCyan.bold\"",
    "devServer": "concurrently \"npm run buildDevServer\"     \"node devServerInit.js --dev server\"      \"npm run buildDevReplApp\"                                  -n \"serv,node,_app\"      -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgCyan.bold\"",
    "dev": "concurrently \"npm run buildDevServer\"     \"node devServerInit.js --dev server\"      \"npm run buildDevApp\"      \"npm run buildDevSw\"          -n \"serv,node,_app,sewo\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgCyan.bold\"",
    "buildAll": "concurrently \"npm run buildProdServer\"    \"npm run buildProdApp\"                    \"npm run buildProdSw\"      \"npm run buildProdReplServer\" -n \"serv,_app,sewo,rese\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgCyan.bold\"",
    "build": "concurrently \"npm run buildProdApp\" \"npm run buildProdSw\" -n \"app,sw\" -c \"bgBlue.bold,bgMagenta.bold\"",
    "start": "wait-on server/dist && node ./server/dist/server.js",
    "startRepl": "wait-on replServer/dist && node ./replServer/dist/server.js",
    "buildProdSw": "rollup --config rollup.sw.prod.config.js",
    "buildProdServer": "rollup --config rollup.server.prod.config.js",
    "buildProdApp": "webpack --config webpack.app.prod.config.js",
    "buildProdReplApp": "webpack --config webpack.replApp.prod.config.js",
    "buildProdReplServer": "rollup --config rollup.replServer.prod.config.js",
    "buildDevSw": "rollup --config rollup.sw.dev.config.js -w",
    "buildDevServer": "rollup --config rollup.server.dev.config.js -w",
    "buildDevApp": "webpack --config webpack.app.dev.config.js",
    "buildDevReplApp": "webpack --config webpack.replApp.dev.config.js",
    "buildDevReplServer": "rollup --config rollup.replServer.dev.config.js -w"
  },
